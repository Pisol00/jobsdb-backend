{
    "compilerOptions": {
      "target": "es2016",
      "module": "commonjs",
      "outDir": "./dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "sourceMap": true,
      "rootDir": "src",
      "noImplicitAny": false,
      "strictNullChecks": false
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }